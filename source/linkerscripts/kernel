INPUT (build/kernel.o)

SECTIONS
{
	. = 0x8000;

	kernel 0x8000:
	{
		entry = .;
		*(.text.kernelentry)
		*(.text*)	
		start_ctors = .;
		*(.ctors*)
		end_ctors = .;
		start_dtors = .;
		*(.dtors*)
		end_dtors = .;
		*(.eh_frame*)
		*(.bss*)
		*(.data*)
		*(.rodata*)
		*(.group*)
		*(.gnu.linkonce*)
	} = 0x0

	/DISCARD/ :
	{
		*(.comment)
	}
}

