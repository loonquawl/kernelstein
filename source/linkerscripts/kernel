SECTIONS
{
	kernel 0x8000:
	{
		entry = .;
		*(.text.kernelentry)
		*(.text*)
		*(.eh_frame*)
		*(.gnu.linkonce*)
		*(.data*)
		*(.rodata*)
		*(.bss*)
	} = 0x0

	ctable ALIGN(8) : 
	{
		constructor_table = .;
		*(.ctors*)
		constructor_table_end = .;
	}

	dtable ALIGN(8) :
	{
		destructor_table = .;
		*(.dtors)
		destructor_table_end = .;
	}

	/DISCARD/ :
	{
		*(.comment*)
	}
}

